<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MetroView">
        <attr name="android:src" />
        <attr name="shadowingWidth" format="dimension"/>
        <attr name="topShadowingWidth" format="dimension"/>
        <attr name="bottomShadowingWidth" format="dimension"/>
        <attr name="leftShadowingWidth" format="dimension"/>
        <attr name="rightShadowingWidth" format="dimension"/>
        <attr name="bgcolor" format="color"/>
        <attr name="hasAnim" format="boolean"/>
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="pointPadding" format="dimension" />
    <attr name="radius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <attr name="metaButtonBarStyle" format="reference" />
    <attr name="metaButtonBarButtonStyle" format="reference" />
    <attr name="gifViewStyle" format="reference" />
    <attr name="gif" format="reference" />
    <attr name="paused" format="boolean" />
    <attr name="layoutManager" format="string" />
    <attr name="spanCount" format="integer" />
    <attr name="reverseLayout" format="boolean" />
    <attr name="stackFromEnd" format="boolean" />
</resources>